   "source": [
    "#복소수 = 실수부(Real number) + 허수부(Imaginary number)\n",
    "#ex) 3 + 4i 실수부:3 허수부: 4\n",
    "a = 3+4j\n",
    "b = 2-3j\n",
    "\n",
    "c = 3 + 4j\n",
    "d = c.real\n",
    "f = c.imag\n",
    "#절대값(Absolute value) 켤레 복소수(Conjugate complex number)\n",
    "e = a.conjugate()\n",
    "g = abs(a)\n",
    "\n",
    "print(a)\n",
    "print(b)\n",
    "print(d)\n",
    "print(f)\n",
    "print(e)\n",
    "print(g)"
   ]
  }
 ],
